using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp3
{
    class Factors_of_count
    {
        static void Main(string[] args)
        {
            Console.WriteLine(FactorsofCount(12));
        }
        public static int FactorsofCount(int num)
        {
            int count = 0;
          
            for (int i = 1; i <= num; i++)
            {
                if (num % i == 0)
                {
                    count++;
                }
            }
            return count;
        }

        

    }
}
